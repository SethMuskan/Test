{
  "extends": ["config:base"],
  "regexManagers": [
    {
      "fileMatch": ["^template/.*\\.yaml$"],
      "matchStrings": [
        "apiVersion: apps/v1beta2"
      ],
      "currentValueTemplate": "v1beta2",
      "depNameTemplate": "Kubernetes",
      "autoReplaceStringTemplate": "apiVersion: v1",
      "datasourceTemplate": "helm"
    }
  ]
}
