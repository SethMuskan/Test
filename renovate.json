{
  "extends": ["config:base"],
  "regexManagers": [
    {
      "fileMatch": ["template/.+\\.yaml$"],
      "matchStrings": [
        "apiVersion: .*"
      ],
      "datasourceTemplate": "kubernetes-api",
      "currentValueTemplate": "apiVersion: apps/v1beta2",
      "depNameTemplate": "{{match}}"
    }
  ],
  "labels": [
    "renovate"
  ]
}
